{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint/eslint-plugin"
    ],
    "env": {
        "node": true,
        "commonjs": true,
        "browser": true
    },
    "rules": {
        "semi": "warn",
        "no-var": "error",
        "no-unused-vars": "warn",
        "no-unused-labels": "warn",
        "no-unused-expressions": "warn",
        "no-console": "warn"
    }
}